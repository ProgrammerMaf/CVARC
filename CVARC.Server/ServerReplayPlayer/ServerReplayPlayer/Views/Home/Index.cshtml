@model ServerReplayPlayer.Models.Log[]

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
    <head>
        <title>Replayer</title>
        <link type="text/css" rel="stylesheet" href="other/css.css" />
        <script lang="text/javascript" src="Scripts/three.js"></script>
        <script lang="text/javascript" src="Scripts/ReplayLoader.js"></script>
        <script lang="text/javascript" src="Scripts/ReplayWindow.js"></script>
        <script lang="text/javascript" src="Scripts/showReplay.js"></script>
        <script src="http://code.jquery.com/jquery-1.7.2.js" type="text/javascript"></script>
        <script lang="text/javascript" src="Scripts/logic.js"></script>
    </head>
    <body onload="onload();" link="red" vlink="#0000ff" alink="#0000ff">
        <div id="scene" style="width: 800px; height: 600px; position: absolute;" class="noselect"></div>
        <table id="info">
            <thead>
                <tr>
                    <td>Имя</td>
                    <td>Язык</td>
                    <td>Очки</td>
                    <td>Время</td>
                </tr>
            </thead>
            @foreach (var a in Model)
            {
                <tr>
                    <td><a wantPlayReplay="true" href="#">@a.Name</a></td>
                    <td>@a.Language</td>
                    <td>@a.Points</td>
                    <td>@a.Time</td>
                </tr>
            }
        </table>
    </body>
</html>